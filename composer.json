{
	"name": "fliglio/chinchilla",
	"description": "Chinchilla",
	"type": "library",
	"license": "GPL-3.0+",
	"bin": [
		"chinchilla"
	],
	"authors": [{
		"name": "Philip Whitt",
		"email": "philipwhitt88@gmail.com",
		"homepage": "http://www.philipwhitt.com"
	}],
	"minimum-stability": "dev",
	"require": {
		"php"                       : ">=5.6",
		"fliglio/web"               : "2.3.*",
		"fliglio/routing"           : "2.3.*",
		"fliglio/flfc"              : "2.3.*",
		"sensiolabs/consul-php-sdk" : "1.*",
		"symfony/yaml"              : "3.4.*",
		"php-amqplib/php-amqplib"   : "2.6.*|2.7.*"
	},
	"require-dev": {
		"phpunit/phpunit"           : "5.*"
	},
	"autoload": {
		"psr-4": {
			"Fliglio\\Chinchilla\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fliglio\\Chinchilla\\" : "test/"
